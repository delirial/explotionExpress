{
  "name": "explosion",
  "version": "1.0.0",
  "description": "Explosions API",
  "repository": "https://github.com/ArcaDelDelirio/explotionExpress",
  "main": "dist/server.js",
  "scripts": {
    "start": "node .",
    "start:database": "mongod & sleep 5 && mongo mongo-init.js && npm run start",
    "build": "chmod +x build.sh; ./build.sh ;npm i ;npm run build:local",
    "build:local": "npx tsc",
    "build:docs": "jsdoc -c jsdoc.config.json",
    "build:dev": "npm run clean && npx tsc",
    "clean": "rm -r ./dist",
    "clean:docs": "rm -r ./docs",
    "clean:all": "npm run clean; npm run clean:docs",
    "dev:watch": "nodemon server.js",
    "dev:ts": "ts-node ./src/server.ts",
    "deploy:netlify": "chmod +x build.sh; ./build.sh ;npm i ;npm run build; npm run serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.4",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.7",
    "@types/node": "^12.0.12",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "better-docs": "^1.1.6",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-prettier": "^3.1.0",
    "express-generator": "^4.16.1",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0"
  }
}
